<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.financefragment"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.financeplanner.activities.ScenarioListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.achartengine.GraphicalActivity" />
        <activity
            android:name="com.example.financeplanner.activities.ScenarioDetailGraphActivity"
            android:label="@string/title_scenario_detail"
            android:parentActivityName="com.example.financeplanner.activities.ScenarioListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ScenarioListActivity" />
        </activity>
        <activity
            android:name="com.example.financeplanner.activities.ScenarioDetailEditActivity"
            android:label="@string/title_scenario_detail"
            android:parentActivityName="com.example.financeplanner.activities.ScenarioListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ScenarioListActivity" />
        </activity>
       
    </application>

</manifest>
